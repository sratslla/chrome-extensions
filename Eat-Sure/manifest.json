{
	"manifest_version": 3,
	"name": "Eat Sure",
	"version": "1.0",
	"description": "Provide info regarding what item is safe for eating",
	"icons": {
		"16": "images/logo_16.png",
		"32": "images/logo_32.png",
		"48": "images/logo_48.png",
		"128": "images/logo_128.png"
	},
	"content_scripts": [
		{
			"js": ["scripts/main.js"],
			"matches": ["https://blinkit.com/prn/*"]
		}
	]
}
